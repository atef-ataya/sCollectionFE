{
    "version": 1,
    "projects": {
        "admin": {
            "projectType": "application",
            "root": "apps/admin",
            "sourceRoot": "apps/admin/src",
            "prefix": "bluebits",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/admin",
                        "index": "apps/admin/src/index.html",
                        "main": "apps/admin/src/main.ts",
                        "polyfills": "apps/admin/src/polyfills.ts",
                        "tsConfig": "apps/admin/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["apps/admin/src/favicon.ico", "apps/admin/src/assets"],
                        "styles": ["apps/admin/src/styles.scss", "node_modules/quill/dist/quill.core.css", "node_modules/quill/dist/quill.snow.css"],
                        "scripts": ["node_modules/quill/dist/quill.js"]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "environments/environment.ts",
                                    "with": "environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "admin:build:production"
                        },
                        "development": {
                            "browserTarget": "admin:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "admin:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/admin/src/**/*.ts", "apps/admin/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/admin"],
                    "options": {
                        "jestConfig": "apps/admin/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "admin-e2e": {
            "root": "apps/admin-e2e",
            "sourceRoot": "apps/admin-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/admin-e2e/cypress.json",
                        "devServerTarget": "admin:serve:development"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "admin:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/admin-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "orders": {
            "projectType": "library",
            "root": "libs/orders",
            "sourceRoot": "libs/orders/src",
            "prefix": "orders",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:ng-packagr-lite",
                    "outputs": ["dist/libs/orders"],
                    "options": {
                        "project": "libs/orders/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/orders/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "libs/orders/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/orders"],
                    "options": {
                        "jestConfig": "libs/orders/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/orders/src/**/*.ts", "libs/orders/src/**/*.html"]
                    }
                }
            }
        },
        "products": {
            "projectType": "library",
            "root": "libs/products",
            "sourceRoot": "libs/products/src",
            "prefix": "bluebits",
            "architect": {
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/products"],
                    "options": {
                        "jestConfig": "libs/products/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/products/src/**/*.ts", "libs/products/src/**/*.html"]
                    }
                }
            }
        },
        "scollection": {
            "projectType": "application",
            "root": "apps/scollection",
            "sourceRoot": "apps/scollection/src",
            "prefix": "bluebits",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "outputPath": "dist/apps/scollection",
                        "index": "apps/scollection/src/index.html",
                        "main": "apps/scollection/src/main.ts",
                        "polyfills": "apps/scollection/src/polyfills.ts",
                        "tsConfig": "apps/scollection/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": ["apps/scollection/src/favicon.ico", "apps/scollection/src/assets"],
                        "styles": ["apps/scollection/src/styles.scss"],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/scollection/src/environments/environment.ts",
                                    "with": "apps/scollection/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "scollection:build:production"
                        },
                        "development": {
                            "browserTarget": "scollection:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "scollection:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["apps/scollection/src/**/*.ts", "apps/scollection/src/**/*.html"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/scollection"],
                    "options": {
                        "jestConfig": "apps/scollection/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "scollection-e2e": {
            "root": "apps/scollection-e2e",
            "sourceRoot": "apps/scollection-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/scollection-e2e/cypress.json",
                        "devServerTarget": "scollection:serve:development",
                        "tsConfig": "apps/scollection-e2e/tsconfig.json"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "scollection:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "outputs": ["{options.outputFile}"],
                    "options": {
                        "lintFilePatterns": ["apps/scollection-e2e/**/*.{js,ts}"]
                    }
                }
            }
        },
        "ui": {
            "projectType": "library",
            "root": "libs/ui",
            "sourceRoot": "libs/ui/src",
            "prefix": "bluebits",
            "architect": {
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/ui"],
                    "options": {
                        "jestConfig": "libs/ui/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
                    }
                }
            }
        },
        "users": {
            "projectType": "library",
            "root": "libs/users",
            "sourceRoot": "libs/users/src",
            "prefix": "users",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:ng-packagr-lite",
                    "outputs": ["dist/libs/users"],
                    "options": {
                        "project": "libs/users/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/users/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "libs/users/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/users"],
                    "options": {
                        "jestConfig": "libs/users/jest.config.js",
                        "passWithNoTests": true
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/users/src/**/*.ts", "libs/users/src/**/*.html"]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "npm"
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "scollection"
}
